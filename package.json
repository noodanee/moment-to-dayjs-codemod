{
    "name": "moment-to-dayjs-codemod",
    "version": "0.0.1",
    "description": "Transforms JS/TS files from moment to dayjs",
    "main": "transform.ts",
    "author": {
        "name": "noodanee",
        "url": "https://github.com/noodanee"
    },
    "license": "MIT",
    "scripts": {
        "build": "tsc transform.ts",
        "test": "NODE_OPTIONS=--experimental-vm-modules jest",
        "transform": "jscodeshift --parser=tsx --extensions=js,jsx,ts,tsx -t transform.ts"
    },
    "keywords": [
        "moment",
        "dayjs",
        "codemod",
        "jscodeshift"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/noodanee/moment-to-dayjs-codemod.git"
    },
    "bugs": {
        "url": "https://github.com/noodanee/moment-to-dayjs-codemod/issues"
    },
    "homepage": "https://github.com/noodanee/moment-to-dayjs-codemod#readme",
    "dependencies": {},
    "devDependencies": {
        "@types/jest": "^29.5.3",
        "@types/jscodeshift": "^0.11.6",
        "@types/moment": "^2.13.0",
        "@typescript-eslint/eslint-plugin": "^6.1.0",
        "@typescript-eslint/parser": "^6.1.0",
        "dayjs": "^1.11.9",
        "eslint": "^8.45.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-prettier": "^5.0.0",
        "jest": "^29.6.1",
        "jscodeshift": "^0.15.0",
        "moment": "^2.29.4",
        "prettier": "^3.0.0",
        "ts-jest": "^29.1.1",
        "ts-node": "^10.9.1",
        "typescript": "^5.1.6"
    }
}
